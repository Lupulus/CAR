package Command;

import java.io.File;

import Main.FtpRequest;

public class CommandRetr extends Command{

	public CommandRetr(FtpRequest ftp, File repertoire) {
		super(ftp);
		process(repertoire);
	}
	
	public void process(File repertoire){
		File source = new File(repertoire.getAbsolutePath() + "/" + filename);
	    File destination = new File("" + "/" + filename);

	    source.renameTo(destination);
	}
	
}
