package Command;

import Request.FtpRequest;

public abstract class Command {
	
	FtpRequest ftp = null;
	
	public Command(FtpRequest ftp){
		this.ftp = ftp;
		Answer answer = new Answer();
	}
	
	public void send(String arg){
		ftp.send(arg);
	}
	
	public boolean process(String arg){
		return true;
	}

}
