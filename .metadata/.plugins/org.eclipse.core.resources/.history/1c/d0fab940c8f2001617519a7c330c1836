package Command;

import java.io.File;

import Request.FtpRequest;

public class CommandList extends Command{

	public CommandList(FtpRequest ftp) {
		super(ftp);
		process();
	}
	
	public void process(){
		String[] files = ftp.getCurrentDirectory.list();
		for(int i = 0; i<files.length; i++){
			ftp.getOut().println(files[i]);
			ftp.getOut().flush();
		}
	}

}
